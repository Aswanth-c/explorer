{
  "name": "celestiascan",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/@modularcloud/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.0.1",
    "react": "^18.2.0",
    "tailwind-config": "^0.0.0",
    "vite": "^4.0.4"
  },
  "packageManager": "npm@8.19.2"
}
